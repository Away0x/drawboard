{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "no-bitwise": [
      false
    ],
    "no-angle-bracket-type-assertion": [
      false
    ],
    "member-ordering": [
      false
    ],
    "variable-name": [
      false
    ],
    "no-console": [
      false
    ],
    "no-empty": [
      false
    ],
    "max-line-length": [
      false
    ],
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": false,
    "prefer-for-of": false,
    "max-classes-per-file": false
  }
}